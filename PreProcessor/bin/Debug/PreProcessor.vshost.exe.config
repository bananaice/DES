<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="algorithm" value="SHA1"/>
    <add key="hash" value="yes"/>
    <add key="filesize" value="65536"/>
    <add key="debuglevel" value="debug"/>
    <add key="replicanum" value="3"/>
    <add key="logfilepath" value="\\Log.txt"/>
  </appSettings>
</configuration>